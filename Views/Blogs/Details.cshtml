@model VendorList.Models.Blog

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4></h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayFor(model => model.CompanyName)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Phone)
            <div>
                @Html.DisplayFor(model => model.StreetNumber)
                <div>
                    @Html.DisplayFor(model => model.StreetName)
                    @Html.DisplayFor(model => model.State)
                    @Html.DisplayFor(model => model.Code)
                </div>
                <div>
                    @Html.DisplayFor(model => model.CompanyCountry)
                </div>
                @Html.DisplayFor(model => model.Noted)  
            </div>
        </dd>
        </dd>
        <dd class="col-sm-10">

        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.BlogId">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
